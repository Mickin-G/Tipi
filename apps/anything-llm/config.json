{
  "$schema": "../schema.json",
  "name": "AnythingLLM",
  "available": true,
  "exposable": true,
  "port": 33001,
  "id": "anything-llm",
  "tipi_version": 1,
  "version": "1.2.0",
  "categories": [
    "utilities",
    "ai"
  ],
  "description": "The all-in-one AI app you were looking for. Chat with your docs, use AI Agents, hyper-configurable, multi-user, & no frustrating set up required.",
  "short_desc": "Your personal LLM trained on anything",
  "author": "Mintplex Labs",
  "source": "https://github.com/Mintplex-Labs/anything-llm",
  "form_fields": [
    {
      "name": "init_account_name",
      "label": "Initial Account Name",
      "type": "text",
      "env_variable": "USERNAME",
      "required": true,
      "placeholder": "iamgroot",
      "min": 2,
      "max": 256
    },
    {
      "name": "init_account_pass",
      "label": "Initial Account Password",
      "type": "password",
      "env_variable": "PASSWORD",
      "required": true,
      "placeholder": "toorgmai",
      "min": 8,
      "max": 250
    },
    {
      "name": "init_llm_provider",
      "label": "Initial LLM Provider",
      "type": "text",
      "env_variable": "LLM_NAME",
      "required": true,
      "options": [
        { "label": "Ollama", "value": "OLLAMA" },
        { "label": "OpenAI", "value": "OPENAI" },
        { "label": "Gemini", "value": "GEMINI" },
        { "label": "Azure", "value": "AZURE" },
        { "label": "Anthropic", "value": "ANTHROPIC" },
        { "label": "LMStudio", "value": "LMSTUDIO" },
        { "label": "LocalAI", "value": "LOCALAI" },
        { "label": "TogetherAI", "value": "TOGETHER" },
        { "label": "Perplexity", "value": "PERPLEXITY" },
        { "label": "OpenRouter", "value": "OPENROUTER" },
        { "label": "Mistral", "value": "MISTRAL" },
        { "label": "HuggingFace", "value": "HUGGINGFACE" },
        { "label": "KoboldCPP", "value": "KOBOLODCPP" },
        { "label": "TextWebGenUI", "value": "TEXTWEBGENUI" },
        { "label": "Generic OpenAI-compatible", "value": "GENOPENAI" },
        { "label": "litellm", "value": "LITELLM" },
        { "label": "Cohere", "value": "COHERE" }
      ]
    },
    {
      "name": "init_llm_api_key",
      "label": "Initial LLM API Key",
      "type": "password",
      "env_variable": "APIKEY",
      "required": true,
      "placeholder": "toorgmai",
      "min": 8,
      "max": 250
    },
    {
      "name": "init_llm_url",
      "label": "Initial LLM URL",
      "type": "text",
      "env_variable": "LLM_URL",
      "required": false,
      "placeholder": "http://127.0.0.1:11434/v1",
      "min": 8,
      "max": 250
    }
  ],
  "supported_architectures": ["amd64","arm64"],
